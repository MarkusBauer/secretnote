{
  "name": "secretnote-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "node prebuild-filter-icons.js && node prebuild.js",
    "build-traditional": "ng build --subresource-integrity --aot=true --prod=true",
    "build": "ng run secretnote-fe:prerender",
    "postbuild": "node postbuild.js",
    "postinstall": "node prebuild.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n-extract-components": "ng build --configuration=all && locl extract -s=\"./tmp/*.js\" -f xlf -o src/locale",
    "i18n-extract-templates": "ng xi18n --output-path src/locale",
    "start-de": "ng serve --configuration=de",
    "dev:ssr": "ng run secretnote-fe:serve-ssr",
    "serve:ssr": "node dist/secretnote-fe/server/main.js",
    "build:ssr": "ng build --prod && ng run secretnote-fe:server:production",
    "prerender": "ng run secretnote-fe:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/localize": "~10.0.14",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/platform-server": "~10.0.14",
    "@angular/router": "~10.0.14",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0-beta.1",
    "@nguniversal/express-engine": "^10.0.2",
    "@primer/octicons": "^11.0.0",
    "@types/sjcl": "^1.0.29",
    "bootstrap": "^4.5.2",
    "express": "^4.15.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "~10.1.2",
    "@angular/compiler-cli": "~10.0.14",
    "@locl/cli": "^1.0.0",
    "@nguniversal/builders": "^10.0.2",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "html-minifier": "^4.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "browser": {
    "crypto": false
  }
}
